{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "start:clear": "expo start --clear",
    "android": "expo start --android",
    "android:clear": "expo start --android --clear",
    "ios": "expo start --ios",
    "ios:clear": "expo start --ios --clear",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "clean": "rm -rf node_modules/.cache .expo && watchman watch-del-all || true",
    "clean:full": "npm run clean && rm -rf node_modules && npm install",
    "verify:symlinks": "ls -la src/shared/types && ls -la src/shared/utils/formatters.ts && ls -la src/shared/lib/validations.ts",
    "debug:metro": "DEBUG_METRO=true expo start --clear"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "^8.4.4",
    "@react-navigation/bottom-tabs": "^7.8.5",
    "@react-navigation/native": "^7.1.20",
    "@react-navigation/stack": "^7.6.4",
    "axios": "^1.13.2",
    "expo": "~54.0.25",
    "expo-local-authentication": "^17.0.7",
    "expo-secure-store": "^15.0.7",
    "expo-status-bar": "~3.0.0",
    "lucide-react-native": "^0.554.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-hook-form": "^7.66.1",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-worklets": "0.5.1",
    "tailwindcss": "^3.3.2",
    "victory-native": "^41.20.2",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "^54.0.7",
    "jest-expo": "^54.0.13",
    "typescript": "~5.9.0"
  },
  "private": true
